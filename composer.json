{
    "name": "thelostasura/connector",
    "authors": [
        {
            "name": "thelostasura",
            "email": "mail@thelostasura.com"
        }
    ],
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "catfan/medoo": "^1.7",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/cache": "^8.21",
        "illuminate/container": "^8.21",
        "illuminate/filesystem": "^8.21",
        "illuminate/http": "^8.21"
    },
    "autoload": {
        "psr-4": {
            "TheLostAsura\\Connector\\": "app/"
        },
        "classmap": [
            "database/migrations/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
